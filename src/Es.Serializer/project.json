{
  "version": "1.0.0",
  "name": "Es.Serializer",
  "title": "Es.Serializer",
  "description": "Easy Serializer",
  "authors": [ "EsWork", "v.la@live.cn" ],
  "packOptions": {
    "summary": "Easy Work Serializer for .NET",
    "owners": [ "v.la@live.cn" ],
    "tags": [ "Serializer" ],
    "projectUrl": "https://github.com/EsWork/Es.Serializer",
    "licenseUrl": "https://github.com/EsWork/Es.Serializer/blob/master/LICENSE",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "git://github.com//EsWork/Es.Serializer"
    }
  },
  "buildOptions": {
    "define": [ "NETCORE" ],
    "debugType": "portable",
    "warningsAsErrors": true,
    "keyFile": "../Es.snk"
  },

  "frameworks": {
    "netstandard1.6": {
      "imports": [
        "dotnet5.6",
        "portable-net451+win8"
      ],
      "dependencies": {
        "NETStandard.Library": "1.6.0-*",
        "System.Runtime.Serialization.Xml": "4.1.0",
        "System.Runtime.Serialization.Formatters": "4.0.0-*",
        "System.Xml.XmlSerializer": "4.0.11-*",
        "System.Private.DataContractSerialization": "4.1.1"
      }
    },
    "net45": {
      "buildOptions": { "define": [ "NET45" ] },
      "frameworkAssemblies": {
        "System": "",
        "System.Core": "",
        "System.Xml": "",
        "System.Runtime.Serialization": "",
        "System.Runtime.Serialization.Formatters.Soap": ""
      }
    }
  }
}
