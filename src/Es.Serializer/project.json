{
  "version": "1.0.3",
  "name": "Es.Serializer",
  "title": "Es.Serializer",
  "description": "Easy Serializer",
  "authors": [ "EsWork", "v.la@live.cn" ],
  "packOptions": {
    "summary": "Easy Work Serializer for .NET",
    "owners": [ "v.la@live.cn" ],
    "tags": [ "Serializer" ],
    "iconUrl": "http://s.gravatar.com/avatar/28dfa5190889f7190e74b683fc30b827?s=80",
    "projectUrl": "https://github.com/EsWork/Es.Serializer",
    "licenseUrl": "https://github.com/EsWork/Es.Serializer/blob/master/LICENSE",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "git://github.com//EsWork/Es.Serializer"
    }
  },
  "buildOptions": {
    "debugType": "portable",
    "xmlDoc": true,
    "warningsAsErrors": true,
    "keyFile": "../Es.snk"
  },

  "frameworks": {
    "netstandard1.3": {
      "buildOptions": { "define": [ "NETSTANDARD" ] },
      "dependencies": {
        "System.Collections": "4.3.0",
        "System.IO": "4.3.0",
        "System.Runtime.Extensions": "4.3.0",
        "System.Threading": "4.3.0",
        "System.Xml.XmlSerializer": "4.3.0",
        "System.Runtime.InteropServices": "4.3.0"
      }
    },
    "net40": {
      "buildOptions": { "define": [ "NETFULL" ] },
      "frameworkAssemblies": {
        "System": "4.0.0.0",
        "System.Core": "4.0.0.0",
        "System.Xml": "4.0.0.0",
        "System.Runtime.Serialization": "4.0.0.0",
        "System.Runtime.Serialization.Formatters.Soap": "4.0.0.0"
      }
    },
    "net45": {
      "buildOptions": { "define": [ "NETFULL" ] },
      "frameworkAssemblies": {
        "System": "4.0.0.0",
        "System.Core": "4.0.0.0",
        "System.Xml": "4.0.0.0",
        "System.Runtime.Serialization": "4.0.0.0",
        "System.Runtime.Serialization.Formatters.Soap": "4.0.0.0"
      }
    }
  }
}
