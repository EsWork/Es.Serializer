{
  "version": "1.0.2",
  "name": "Es.Serializer",
  "title": "Es.Serializer",
  "description": "Easy Serializer",
  "authors": [ "EsWork", "v.la@live.cn" ],
  "packOptions": {
    "summary": "Easy Work Serializer for .NET",
    "owners": [ "v.la@live.cn" ],
    "tags": [ "Serializer" ],
    "projectUrl": "https://github.com/EsWork/Es.Serializer",
    "licenseUrl": "https://github.com/EsWork/Es.Serializer/blob/master/LICENSE",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "git://github.com//EsWork/Es.Serializer"
    }
  },
  "buildOptions": {
    "debugType": "portable",
    "xmlDoc": true,
    "warningsAsErrors": true,
    "keyFile": "../Es.snk"
  },


  "runtimes": {
    "win7-x64": {},
    "win10-x64": {},
    "win81-x64": {},
    "centos.7-x64": {}
  },

  "frameworks": {
    "netstandard1.6": {
      "buildOptions": { "define": [ "NETSTANDARD" ] },
      "dependencies": {
        "NETStandard.Library": "1.6.1-*",
        "System.Xml.XmlSerializer": "4.3.0-*"
      }
    },
    "net45": {
      "buildOptions": { "define": [ "NETFULL" ] },
      "frameworkAssemblies": {
        "System": "4.0.0.0",
        "System.Core": "4.0.0.0",
        "System.Xml": "4.0.0.0",
        "System.Runtime.Serialization": "4.0.0.0",
        "System.Runtime.Serialization.Formatters.Soap": "4.0.0.0"
      }
    }
  }
}
