{
  "buildOptions": {
    "emitEntryPoint": true
  },

  "dependencies": {
    "protobuf-net": "2.1.0-*",
    "Es.Serializer": "1.0.2-*",
    "Es.Serializer.ProtoBuf": "1.0.2-*",
    "Es.Serializer.JsonNet": "1.0.2-*"
  },

  "frameworks": {
    "netcoreapp1.1": {
      "imports": "dnxcore50",
      "buildOptions": { "define": [ "NETSTANDARD" ] },
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0-*"
        }
      }
    },
    "net45": {
      "buildOptions": { "define": [ "NETFULL" ] },
      "dependencies": {
        "Es.Serializer.NetSerializer": "1.0.2-*",
        "Es.Serializer.Jil": "1.0.2-*"
      },
      "frameworkAssemblies": {
        "System": "4.0.0.0",
        "System.Core": "4.0.0.0",
        "System.Xml": "4.0.0.0",
        "System.Runtime": "4.0.0.0",
        "System.Runtime.Serialization": "4.0.0.0",
        "System.Runtime.Serialization.Formatters.Soap": "4.0.0.0"
      }
    }
  }
}
